    "@types/node-fetch": "^2.1.1",
    "@types/whatwg-url": "^6.4.0",
    "jest": "^23.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "prettier-check": "^2.0.0",
